<h1 class="my-4 text-center">Users</h1>
<div class="container">
  <table class="table table-striped table-hover">
    <thead class="thead-dark">
      <tr>
        <th>Name</th>
        <th>Email</th>
        <th>Role</th>
        <th>Created At</th>
      </tr>
    </thead>
    <tbody id="users-table-tbody">
      <%= render partial: "users/users_list", locals: { users: @users } %>
    </tbody>
  </table>  
</div>

